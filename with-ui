# ==========================================

# ðŸŸ¦ CONTROLLER

# ==========================================

<<<FILE: src/main/java/com/example/generator/controller/ProjectController.java>>>
package com.example.generator.controller;

import com.example.generator.service.ProjectGeneratorService;
import jakarta.servlet.http.HttpServletResponse;
import lombok.RequiredArgsConstructor;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.*;

import java.io.IOException;

@Controller
@RequiredArgsConstructor
public class ProjectController {

```
private final ProjectGeneratorService generatorService;

@PostMapping("/generate")
public void generate(
        @RequestParam String groupId,
        @RequestParam String artifactId,
        @RequestParam String packageName,
        @RequestParam(defaultValue = "1.0.0") String version,
        HttpServletResponse response
) throws IOException {

    byte[] zip = generatorService.generateAsZip(groupId, artifactId, packageName, version);

    response.setContentType("application/zip");
    response.setHeader("Content-Disposition",
            "attachment; filename=" + artifactId + ".zip");

    response.getOutputStream().write(zip);
}
```

}
<<<END FILE>>>

---

# ==========================================

# ðŸŸ§ SERVICE LAYER

# ==========================================

<<<FILE: src/main/java/com/example/generator/service/ProjectGeneratorService.java>>>
package com.example.generator.service;

import com.example.generator.util.ZipUtil;
import freemarker.template.Configuration;
import freemarker.template.Template;
import org.springframework.stereotype.Service;

import java.io.*;
import java.nio.file.*;
import java.util.*;

@Service
public class ProjectGeneratorService {

```
private final Configuration cfg;

public ProjectGeneratorService() throws IOException {
    cfg = new Configuration(Configuration.VERSION_2_3_9);
    cfg.setClassLoaderForTemplateLoading(getClass().getClassLoader(), "templates");
}

public byte[] generateAsZip(String groupId, String artifactId,
                            String packageName, String version)
        throws IOException {

    Path tempDir = Files.createTempDirectory("generated-");

    Map<String, Object> data = Map.of(
            "groupId", groupId,
            "artifactId", artifactId,
            "packageName", packageName,
            "version", version
    );

    Path root = tempDir.resolve(artifactId);
    Files.createDirectories(root);

    generate("pom.xml.ftl", root.resolve("pom.xml"), data);
    generate("README.md.ftl", root.resolve("README.md"), data);

    Path javaDir = root.resolve("src/main/java/" + packageName.replace('.', '/'));
    Path resources = root.resolve("src/main/resources");

    Files.createDirectories(javaDir);
    Files.createDirectories(resources);

    generate("Application.ftl",
            javaDir.resolve(cap(artifactId) + "Application.java"), data);

    generate("application.properties.ftl",
            resources.resolve("application.properties"), data);

    // releng
    Path releng = root.resolve("releng");
    Files.createDirectories(releng.resolve("build"));
    Files.createDirectories(releng.resolve("packaging/docker"));
    Files.createDirectories(releng.resolve("packaging/zip"));
    Files.createDirectories(releng.resolve("version"));

    generate("releng/build/build.sh.ftl", releng.resolve("build/build.sh"), data);
    generate("releng/build/build.ps1.ftl", releng.resolve("build/build.ps1"), data);
    generate("releng/packaging/docker/Dockerfile.ftl", releng.resolve("packaging/docker/Dockerfile"), data);
    generate("releng/packaging/zip/package.sh.ftl", releng.resolve("packaging/zip/package.sh"), data);
    generate("releng/version/version.txt.ftl", releng.resolve("version/version.txt"), data);

    return ZipUtil.zipDirectoryToBytes(root);
}

private void generate(String templateName, Path target, Map<String, Object> data)
        throws IOException {
    Template t = cfg.getTemplate(templateName);
    try (Writer w = Files.newBufferedWriter(target)) {
        t.process(data, w);
    } catch (Exception e) {
        throw new RuntimeException(e);
    }
}

private String cap(String value) {
    return Character.toUpperCase(value.charAt(0)) + value.substring(1);
}
```

}
<<<END FILE>>>

---

<<<FILE: src/main/java/com/example/generator/util/ZipUtil.java>>>
package com.example.generator.util;

import java.io.*;
import java.nio.file.*;
import java.util.zip.*;

public class ZipUtil {

```
public static byte[] zipDirectoryToBytes(Path sourceDir) throws IOException {
    ByteArrayOutputStream output = new ByteArrayOutputStream();
    ZipOutputStream zip = new ZipOutputStream(output);

    Files.walk(sourceDir).forEach(path -> {
        try {
            if (Files.isDirectory(path)) return;

            String entryName = sourceDir.relativize(path).toString();
            zip.putNextEntry(new ZipEntry(entryName));
            Files.copy(path, zip);
            zip.closeEntry();
        } catch (IOException e) {
            throw new RuntimeException(e);
        }
    });

    zip.close();
    return output.toByteArray();
}
```

}
<<<END FILE>>>

